{
  "name": "material.layouts",
  "version": "0.1.0",
  "description": "Angular Layouts javascript library for super-easy flexbox features",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular/material-layouts.git"
  },
  "bugs": "https://github.com/angular/material-layouts/issues",
  "authors": [
    {
      "name": "Thomas Burleson",
      "email": "ThomasBurleson@gmail.com",
      "url": "http://solutionoptimist.com"
    },
    {
      "name": "Ryan Schmukler",
      "email": "ryan@slingingcode.com",
      "url": "http://slingingcode.com"
    }
  ],
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "postinstall": "typings install",
    "copy-files": "rm -fr ./dist; mkdir ./dist; cp -R ./src/demos ./dist/demos",
    "demo": "npm run copy-files; npm run compile; open http://localhost:8888/demos/; http-server -p 8888 ./dist;",
    "demo:watch": "node watch.js",
    "test": "karma start ./config/karma.simple.js",
    "autotest": "karma start --auto-watch --no-single-run ./config/karma.simple.js",
    "bundle": "npm run copy-files; npm run compile"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/material/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "gulp": "^3.9.1",

    "core-js": "^2.1.5",
    "rxjs": "5.0.0-beta.4",
    "zone.js": "0.6.10",
    "clone": "^1.0.2",
    "graceful-fs": "^4.0.0",
    "lodash": "^4.0.0",

    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",

    "ts-loader": "^0.8.1",
    "typescript": "^1.8.9",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "fs-extra": "^0.26.7",
    "livereload": "^0.4.1"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "material",
    "layouts",
    "babel",
    "systemjs"
  ]
}
